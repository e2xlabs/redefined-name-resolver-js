{
  "name": "@redefined/name-resolver-js",
  "version": "1.2.5",
  "description": "JS lib to resolve crypto names",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist/**/*"],
  "repository": {
    "type": "git",
    "url": "https://github.com/e2xlabs/redefined-name-resolver-js.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "release": {
    "branches": [
      "master"
    ],
    "access": "public"
  },
  "author": "Cifrex",
  "scripts": {
    "commit": "git-cz",
    "start": "del-cli --force dist && tsc && node dist/index.js",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "semantic-release": "semantic-release",
    "test:unit": "jest"
  },
  "dependencies": {
    "@unstoppabledomains/resolution": "^8.4.0",
    "lodash": "^4.17.21",
    "js-sha256": "^0.9.0",
    "moment": "^2.29.1",
    "web3": "^1.8.1"
  },
  "devDependencies": {
    "typescript": "^4.5.2",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.184",
    "@types/web3": "^1.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.2",
    "semantic-release": "^20.1.0",
    "ts-jest": "^29.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
